// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Topics {
  id   String  @id
  title String
  description String
  published String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

}